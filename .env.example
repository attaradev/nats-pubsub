# NatsPubsub Environment Configuration Example
# Copy this file to .env and update with your actual values

# NATS Server Configuration
NATS_URLS=nats://localhost:4222
NATS_ENV=development
APP_NAME=my-app

# Consumer Configuration
CONCURRENCY=5
MAX_DELIVER=3
ACK_WAIT=30s
BACKOFF=1s,5s,15s,30s,60s

# Features
USE_DLQ=true
USE_OUTBOX=false
USE_INBOX=false

# Database Configuration (for Ruby Inbox/Outbox)
DATABASE_URL=postgresql://user:password@localhost:5432/nats_pubsub_dev

# Logging
LOG_LEVEL=info

# Application Environment
NODE_ENV=development
RAILS_ENV=development
