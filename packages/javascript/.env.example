# JavaScript/TypeScript NatsPubsub Environment Configuration
# Copy this file to .env and update with your actual values

# NATS Server Configuration
NATS_URLS=nats://localhost:4222
NODE_ENV=development
APP_NAME=javascript-app

# Consumer Configuration
CONCURRENCY=10
MAX_DELIVER=5
ACK_WAIT=30000
BACKOFF=1000,5000,15000,30000,60000

# Features
USE_DLQ=true

# Logging
LOG_LEVEL=info

# Application Port (if running as web service)
PORT=3000
