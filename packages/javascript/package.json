{
  "name": "nats-pubsub",
  "version": "0.1.0",
  "description": "Declarative PubSub messaging for NATS JetStream (Node.js/TypeScript)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --out docs src/index.ts",
    "docs:watch": "typedoc --watch --out docs src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nats",
    "jetstream",
    "pubsub",
    "messaging",
    "event-driven",
    "microservices"
  ],
  "author": "Mike Attara <mpyebattara@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/attaradev/nats_pubsub.git",
    "directory": "packages/javascript"
  },
  "bugs": {
    "url": "https://github.com/attaradev/nats_pubsub/issues"
  },
  "homepage": "https://github.com/attaradev/nats_pubsub#readme",
  "dependencies": {
    "nats": "^2.28.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "typedoc": "^0.28.14",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  }
}
